{
    "name": "swinde/contao-hello-world-bundle",
    "type": "contao-bundle",
    "description": "Hello World extension for Contao Open Source CMS",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Steffen Winde",
            "homepage": "https://github.com/swinde"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "4.4.*",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "contao/easy-coding-standard": "^2.0",
        "phpunit/phpunit": "^8.4",
        "symfony/phpunit-bridge": "^4.4 || ^5.1"
    },
    "extra": {
        "contao-manager-plugin": "Swinde\\ContaoHelloWorldBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Swinde\\ContaoHelloWorldBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swinde\\ContaoHelloWorldBundle\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/swinde/contao-hello-world-bundle/issues",
        "source": "https://github.com/swinde/contao-hello-world-bundle"
    }
}
